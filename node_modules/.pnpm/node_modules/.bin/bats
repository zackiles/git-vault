#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/bats@1.11.1/node_modules/bats/bin/node_modules:/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/bats@1.11.1/node_modules/bats/node_modules:/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/bats@1.11.1/node_modules:/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/bats@1.11.1/node_modules/bats/bin/node_modules:/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/bats@1.11.1/node_modules/bats/node_modules:/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/bats@1.11.1/node_modules:/Users/zacharyiles/dev/git-vault/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../bats/bin/bats" "$@"
else
  exec bash  "$basedir/../bats/bin/bats" "$@"
fi
